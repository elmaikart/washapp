generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id         String   @id @default(cuid())
  service    String
  date       DateTime
  timeSlot   String
  address    String
  notes      String?
  createdAt  DateTime @default(now())
}
